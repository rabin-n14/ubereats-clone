{
  "expo": {
    "name": "Rabin UberEats Clone",
    "slug": "rabin-ubereats-clone",
    "owner": "portfolios",
    "version": "1.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.expo.client"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "React Native UberEats app made by Rabin on Expo App. Unfortunately due to Privacy Policy of Apple, app can't be made public. However, reach out to Rabin with your Signed Up Expo Email so he can let access to view project in your iOS device."
  }
}
